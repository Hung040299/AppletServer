{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "prettier": true,
        "no-console": false,
        "no-var-requires": false,
        "object-literal-shorthand": false,
        "object-literal-sort-keys": false,
        "quotemark": true,
        "member-access": false,
        "no-shadowed-variable": false
    },
    "rulesDirectory": [
        "tslint-plugin-prettier"
    ]
}